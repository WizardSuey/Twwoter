<div class="ProfileHeader">
    <%= form_with model: @post, :html => { :multipart => true }, data: {turbo: false} do |form| %>
        <%= render "shared/form_errors", form: form %>
        <%= form.label :likes_count %>
        <%= form.text_field :likes_count, class: 'form-control' %>
        <div class="mt-3">
            <%= form.submit 'Изменить', class: 'btn btn-primary' %>
        </div>
    <% end %>
</div>
